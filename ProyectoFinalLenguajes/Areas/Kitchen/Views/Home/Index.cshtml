@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <div class="text-center row">
        <h1>Welcome Back!</h1>
        <p>Please Log In!</p>

        <div class="d-flex justify-content-center mx-auto">
            <input placeholder="Your Email" type="email" class="w-25  form-control " />
            <hr />
        </div>
        <div class="d-flex justify-content-center mx-auto m-2">
            <input placeholder="Your Password" type="password" class="w-25  form-control " />
        </div>
        <div class="d-flex justify-content-center mx-auto m-1">
            <input type="button" name="name" value="Log In" class="btn btn-primary" />
        </div>
    </div>
</div>
